@model IEnumerable<ReturnTaskDto>

<h2>All Tasks</h2>

<table class="table">
    <thead>
        <tr>
            <th>TaskId</th>
            <th>Title</th>
            <!-- Add other properties you want to display -->
        </tr>
    </thead>
    <tbody>
        @foreach (var task in Model)
        {
            <tr>
                <td>@task.TaskId</td>
                <td>@task.Title</td>
                <!-- Add other properties you want to display -->
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create" class="btn btn-primary">Create New Task</a>
